{
    "plugins": ["prettier"],
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "prettier"
    ],
    "rules": {
        "vue/no-unused-vars": "error"
    },
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "sourceType": "module",
        "requireConfigFile": false
    }
}
